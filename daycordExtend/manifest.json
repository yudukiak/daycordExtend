{
  "manifest_version": 3,
  "name": "DaycordExtend",
  "version": "0.0.1",
  "description": "Daycord の日程表示を見やすく拡張します。",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://character-sheets.appspot.com/schedule/user*"],
      "js": ["js/common.js", "js/user_ps_table.js"],
      "css": ["css/common.css", "css/user_ps_table.css"]
    },
    {
      "matches": ["https://character-sheets.appspot.com/schedule/list?key=*"],
      "js": ["js/common.js", "js/list.js"],
      "css": ["css/common.css", "css/list.css"]
    }
  ],
  "permissions": []
}
